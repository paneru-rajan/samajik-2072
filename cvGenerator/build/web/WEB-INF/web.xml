<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>writeToDocx</servlet-name>
        <servlet-class>cvWriter.writeToDocx</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>writeObj</servlet-name>
        <servlet-class>cvWriter.writeObj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>writeEdu</servlet-name>
        <servlet-class>cvWriter.writeEdu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>writeExp</servlet-name>
        <servlet-class>cvWriter.writeExp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>writeRef</servlet-name>
        <servlet-class>cvWriter.writeRef</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>writeToDocx</servlet-name>
        <url-pattern>/writeToDocx</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>writeObj</servlet-name>
        <url-pattern>/writeObj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>writeEdu</servlet-name>
        <url-pattern>/writeEdu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>writeExp</servlet-name>
        <url-pattern>/writeExp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>writeRef</servlet-name>
        <url-pattern>/writeRef</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
